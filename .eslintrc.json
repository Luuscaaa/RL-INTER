{
  "extends": ["airbnb", "prettier"],
  "plugins": ["react", "react-hooks", "jsx-a11y", "prettier"],
  "env": {
    "browser": true,
    "es2021": true,
    "jest": true
  },
  "rules": {
    "prettier/prettier": "error",
    "react/react-in-jsx-scope": "off",
    "react/prop-types": "error",
    "no-console": "warn"
  }
}
